create database Registrationformuser;
use Registrationformuser;
CREATE TABLE RagisterUserData (
    id INT AUTO_INCREMENT PRIMARY KEY,
    first_name VARCHAR(50) NOT NULL,
    middle_name VARCHAR(50),
    last_name VARCHAR(50) NOT NULL,
    email VARCHAR(100) NOT NULL,
    address VARCHAR(255),
    age INT,
    sex VARCHAR(10),
    city VARCHAR(50)
);
describe RagisterUserData;
select*from RagisterUserData;
